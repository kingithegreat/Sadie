# SADIE SSE Proxy example environment variables
# Required
PORT=5050
PROXY_API_KEYS=changeme
PROXY_ADMIN_KEY=adminchangeme
# OpenAI server key (if ENFORCE_SERVER_AUTH is true, this will be used)
OPENAI_API_KEY=
OLLAMA_API_KEY=
# Optional persistence of proxy keys (proxy.config.json encryption)
KEY_ENCRYPTION_SECRET=
# Redis connection - needed for rate-limiting in production
REDIS_URL=redis://redis:6379
# Enforce server-side model credentials
ENFORCE_SERVER_AUTH=false
# Rate limit defaults
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000
# CORS
CORS_ORIGINS=*
# Upstream configs
OLLAMA_URL=http://ollama:8080
OPENAI_API_BASE=https://api.openai.com/v1
