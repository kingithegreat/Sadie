{
  "name": "sadie-sse-proxy",
  "version": "0.1.0",
  "description": "SADIE SSE Proxy - forwards model streaming responses (OpenAI / Ollama) to clients via SSE",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test:integration": "node dist/test/run-integration.js",
    "test:ci": "docker compose -f docker-compose.yml up -d redis mock-upstream && npm run build && npm run test:integration && docker compose -f docker-compose.yml down",
    "test": "jest --runInBand",
    "lint": "eslint . --ext .ts"
  },
  "author": "sadie-team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "undici": "^6.11.0",
    "dotenv": "^16.1.4"
    ,"ioredis": "^5.3.2"
    ,"ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.12",
    "@types/jest": "^29.5.5",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.7.2",
    "@types/ws": "^8.5.4",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.12",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.6.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "eslint": "^8.45.0"
    ,"@types/ws": "^8.5.4"
  }
}